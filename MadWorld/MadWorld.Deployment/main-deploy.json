  "contentVersion": "1.0.0.0",
  "parameters": {
    "regionLocation": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Location for all resources."
      }
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "StaticWebsiteDeployment",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "relativePath": "static-website-deploy.json"
        }
      }
    },
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "BlobsDeployment",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "relativePath": "blobs-deploy.json"
        }
      }
    }
  ],
  "outputs": {
  }
}